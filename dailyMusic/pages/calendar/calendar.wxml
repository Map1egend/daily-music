<!--pages/calendar/calendar.wxml-->
<view class="calendar-container">
  <picker mode="date" value="{{currentDay}}" start="2022-08-30" end="2022-09-10" bindchange="bindDateChange">
    <view class="picker">
      {{ currentDay }}
    </view>
  </picker>
  <music date="{{ currentDay }}"></music>
</view>
